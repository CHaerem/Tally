{
  "name": "norwegian-stock-portfolio-tracker",
  "version": "1.0.0",
  "description": "Track Norwegian stock portfolio performance with dividends",
  "scripts": {
    "dev": "vite",
    "dev:v2": "vite --config vite.config.v2.js",
    "build": "tsc && vite build",
    "build:v2": "tsc && vite build --config vite.config.v2.js",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "deploy": "npm run build && gh-pages -d dist",
    "fetch:test": "node scripts/fetch-stock-data-v2.js --symbols=EQNR,DNB,SNTIA",
    "fetch:full": "node scripts/fetch-stock-data-v2.js --full-update",
    "fetch:dividends": "node scripts/fetch-stock-data-v2.js --dividends-only"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "gh-pages": "^6.1.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "chart.js": "^4.4.0"
  }
}